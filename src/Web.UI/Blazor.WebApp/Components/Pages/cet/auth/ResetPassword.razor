@page "/some-page"
@inject NavigationManager NavigationManager

<button @onclick="GoToTwoFactorPage">Go to Two Factor Authentication</button>

@code {
    private void GoToTwoFactorPage()
    {
        string email = "example@example.com"; // Giá trị email bạn muốn truyền
        NavigationManager.NavigateTo($"/two-factor-authentication?email={Uri.EscapeDataString(email)}");
    }
}
